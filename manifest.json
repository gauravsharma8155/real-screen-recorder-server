{
  "name": "Real Screen Recorder",
  "description": "Real Screen Recorder records the screen and webcam with audio.11111",
  "version": "1.0.2",
  "manifest_version": 3,
  "permissions": ["activeTab","gcm","notifications","unlimitedStorage", "storage", "tabs"],
  "host_permissions": ["*://*/*"],

  "action": {
    "default_title": "Real Screen Recorder"
  },

  "icons": {
    "16": "./image/icon-16.png",
    "24": "./image/icon-24.png",
    "32": "./image/icon-32.png",
    "48": "./image/icon-48.png",
    "64": "./image/icon-64.png",
    "96": "./image/icon-96.png",
    "128": "./image/icon-128.png"
  },

  "background": {
    "service_worker": "background.js"
  }
}
